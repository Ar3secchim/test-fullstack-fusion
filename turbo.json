{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": [
        ".env"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "env": [
        ".env"
      ]
    },
    "start": {
      "dependsOn": ["^build"],
      "env": [".env"]
    }
  },
  "globalEnv": [".env"]
}